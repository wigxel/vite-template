    "postcss": "^8.4.33",
    "postman-to-openapi": "^3.0.1",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.4.1",
    "tailwindcss": "^3.4.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write ./src",
      "npx biome format --no-errors-on-unmatched --write ./src",
      "npx biome lint --apply ./src"
    ],
    "*.{css,html,md,mdx,yml,yaml}": [
      "prettier --write ./src"
    ]
  }
