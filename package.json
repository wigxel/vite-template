    "rxjs": "^7.8.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "ufo": "^1.3.2",
    "uncrypto": "^0.1.3",
    "usehooks-ts": "^2.9.1",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.4"
